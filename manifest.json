{

    "name": "CryptoTrakker",
    "version": "0.29",
    "manifest_version": 2,

    "description": "Track your favorite Crypto currency movement!",
    "icons": {
        "19": "images/cryptocoin.png",
        "38": "images/cryptocoin.png",
        "128": "images/cryptocoin.png" 
    },

    "browser_action": {
        "default_title": "CryptoTrakker",
        "default_icon": {
            "19": "images/cryptocoin.png", 
            "38": "images/cryptocoin.png"
        },
        "default_popup": "html/main.html"
    },


    "options_page": "html/options.html",
    "homepage_url": "https://github.com/dinhnerz/CryptoTrakker#-cryptotrakker",

       "permissions": [
          "storage"
    ]
    
}
